/dev/mapper/centos-root /                       xfs     defaults        1 1
UUID=bcb25739-0dfe-4490-bc88-c4a2caada6ba /boot                   xfs     defaults        1 2
/dev/mapper/centos-home /home                   xfs     defaults        1 2
/dev/mapper/centos-swap swap                    swap    defaults        0 0
